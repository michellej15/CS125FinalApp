<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.michellejun.cs125finalapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".start_game">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DogOrCat"
            android:label="@string/title_activity_start_game"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".end_game"
            android:label="@string/title_activity_end_game"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>